java.math.BigDecimal numerator, denominator, result;String value = "";// BigDecimal records the strings precisionvalue = new java.math.BigDecimal("10.1000000").toString();System.out.println();System.out.println("Preserved Precision");System.out.println(value);//This is incorrect! 1000 is really only 1 sig fig//proper 4 sig figs is 1000.value = new java.math.BigDecimal("1000").toString();System.out.println();System.out.println("Incorrect Precision");System.out.println(value);System.out.println(new java.math.BigDecimal("1000").precision());//Is this correct? 1000/100.0 is 10.00 or 10//because 1000 is really only 1 sig fig//proper 4 sig figs is 1000.numerator = new java.math.BigDecimal("1000");denominator = new java.math.BigDecimal("100.001");result = gov.usgs.wma.statistics.logic.SigFigMathUtil.sigFigDivide(numerator,denominator);System.out.println();System.out.println("But maybe correct for GWW needs? 1000/100.001");System.out.println(result.toString());System.out.println(result.precision());//Correct. 1000. is really 4 sig figvalue = new java.math.BigDecimal("1000.").toString();System.out.println();System.out.println("Correct Precision");System.out.println("1000.");System.out.println(new java.math.BigDecimal("1000.").precision());// this is correct. The sig fig module proper preserves the minimum sif figsnumerator = new java.math.BigDecimal("10.1000");denominator = new java.math.BigDecimal("100.000000001");result = gov.usgs.wma.statistics.logic.SigFigMathUtil.sigFigDivide(numerator,denominator);System.out.println();System.out.println("Division Precision");System.out.println(result.toString());System.out.println(result.precision());//This is incorrect! 1000/100.00 is 10.00 not 1E+1numerator = new java.math.BigDecimal("1000.");denominator = new java.math.BigDecimal("100.00");result = gov.usgs.wma.statistics.logic.SigFigMathUtil.sigFigDivide(numerator,denominator);System.out.println();System.out.println("Incorrect! 1000./100.00 should be 10.00");System.out.println(result.toString());System.out.println(result.precision());numerator = new java.math.BigDecimal("53.1").divide(new java.math.BigDecimal("100"));result = numerator.setScale(10);System.out.println();System.out.println("Set Precision");System.out.println(numerator.toString());System.out.println(numerator.precision());System.out.println(result.toString());System.out.println(result.precision());